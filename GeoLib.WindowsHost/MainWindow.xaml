<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoLib.WindowsHost"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical">
        <Button x:Name="btnStart" Content="Start Services" Margin="40,40" Click="btnStart_Click"></Button>
        <Button x:Name="btnStop" Content="Stop Services" Margin="50,50" Click="btnStop_Click"></Button>
    </StackPanel>
</Window>
